{
  "name": "@snaplet/seed",
  "type": "module",
  "version": "0.92.2",
  "sideEffects": false,
  "bin": {
    "snaplet-seed": "./bin/cli.js"
  },
  "author": {
    "name": "Snaplet",
    "url": "https://snaplet.dev"
  },
  "engines": {
    "node": ">=18.5.0"
  },
  "files": [
    "bin",
    "dist",
    "scripts/postinstall.js",
    "LICENSE.md",
    "README.md"
  ],
  "description": "An auto-generated ORM-like library for generating seed data tailored to your own database",
  "homepage": "https://snaplet.dev/seed",
  "license": "FSL-1.1-MIT",
  "imports": {
    "#*": {
      "development": "./src/*",
      "default": "./dist/*"
    },
    "#test": "./test/index.js",
    "#test/*": "./test/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./adapter": {
      "types": "./dist/core/databaseClient.d.ts",
      "development": "./src/core/databaseClient.ts",
      "default": "./dist/core/databaseClient.js"
    },
    "./adapter-better-sqlite3": {
      "types": "./dist/adapters/better-sqlite3/index.d.ts",
      "development": "./src/adapters/better-sqlite3/index.ts",
      "default": "./dist/adapters/better-sqlite3/index.js"
    },
    "./adapter-pg": {
      "types": "./dist/adapters/pg/index.d.ts",
      "development": "./src/adapters/pg/index.ts",
      "default": "./dist/adapters/pg/index.js"
    },
    "./adapter-postgres": {
      "types": "./dist/adapters/postgres/index.d.ts",
      "development": "./src/adapters/postgres/index.ts",
      "default": "./dist/adapters/postgres/index.js"
    },
    "./adapter-prisma": {
      "types": "./dist/adapters/prisma/index.d.ts",
      "development": "./src/adapters/prisma/index.ts",
      "default": "./dist/adapters/prisma/index.js"
    },
    "./config": {
      "types": "./dist/config/seedConfig/defineConfig.d.ts",
      "development": "./src/config/seedConfig/defineConfig.ts",
      "default": "./dist/config/seedConfig/defineConfig.js"
    },
    "./dialects/postgres/client": {
      "types": "./dist/dialects/postgres/client.d.ts",
      "development": "./src/dialects/postgres/client.ts",
      "default": "./dist/dialects/postgres/client.js"
    },
    "./dialects/sqlite/client": {
      "types": "./dist/dialects/sqlite/client.d.ts",
      "development": "./src/dialects/sqlite/client.ts",
      "default": "./dist/dialects/sqlite/client.js"
    },
    "./core/symbols": {
      "types": "./dist/core/symbols.d.ts",
      "development": "./src/core/symbols.ts",
      "default": "./dist/core/symbols.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "adapter": [
        "./dist/core/databaseClient.d.ts"
      ],
      "adapter-better-sqlite3": [
        "./dist/adapters/better-sqlite3/index.d.ts"
      ],
      "adapter-pg": [
        "./dist/adapters/pg/index.d.ts"
      ],
      "adapter-postgres": [
        "./dist/adapters/postgres/index.d.ts"
      ],
      "adapter-prisma": [
        "./dist/adapters/prisma/index.d.ts"
      ],
      "config": [
        "./dist/config/seedConfig/defineConfig.d.ts"
      ],
      "dialects/postgres/client": [
        "./dist/dialects/postgres/client.d.ts"
      ],
      "dialects/sqlite/client": [
        "./dist/dialects/sqlite/client.d.ts"
      ],
      "core/symbols": [
        "./dist/core/symbols.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@snaplet/copycat": "5.0.0",
    "@snaplet/eslint-config": "workspace:*",
    "@snaplet/tsconfig": "workspace:*",
    "@types/better-sqlite3": "7.6.9",
    "@types/debug": "4.1.12",
    "@types/fs-extra": "11.0.4",
    "@types/node": "20.12.6",
    "@types/pg": "8.11.5",
    "@types/semver": "^7.5.8",
    "@types/uuid": "9.0.8",
    "@types/yargs": "17.0.32",
    "ansi-colors": "4.1.3",
    "better-sqlite3": "9.4.5",
    "dedent": "1.5.1",
    "eslint": "8.57.0",
    "pg": "8.11.5",
    "postgres": "3.4.4",
    "semver": "^7.6.0",
    "tmp-promise": "3.0.3",
    "tsc-alias": "^1.8.8",
    "tsx": "4.7.2",
    "typescript": "5.4.4",
    "vitest": "1.4.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=5",
    "@snaplet/copycat": ">=2",
    "@types/better-sqlite3": "*",
    "@types/pg": "*",
    "better-sqlite3": ">=9",
    "pg": ">=8",
    "postgres": ">=3"
  },
  "peerDependenciesMeta": {
    "@prisma/client": {
      "optional": true
    },
    "@types/better-sqlite3": {
      "optional": true
    },
    "@types/pg": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "postgres": {
      "optional": true
    }
  },
  "dependencies": {
    "@inquirer/prompts": "^4.3.2",
    "@scaleleap/pg-format": "^1.0.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "ansi-escapes": "^6.2.1",
    "c12": "^1.10.0",
    "change-case": "^5.4.4",
    "ci-info": "^4.0.0",
    "debug": "^4.3.4",
    "dedent": "^1.5.1",
    "deepmerge": "^4.3.1",
    "execa": "^8.0.1",
    "exit-hook": "^4.0.0",
    "find-up": "^7.0.0",
    "fs-extra": "^11.2.0",
    "inflection": "^3.0.0",
    "javascript-stringify": "^2.1.0",
    "json-schema-library": "^9.3.4",
    "kleur": "^4.1.5",
    "multimatch": "^7.0.0",
    "ora": "^8.0.1",
    "portfinder": "^1.0.32",
    "posthog-node": "^4.0.0",
    "quicktype-core": "^23.0.115",
    "remeda": "^1.58.0",
    "terminal-link": "^3.0.0",
    "uuid": "^9.0.1",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "scripts": {
    "postinstall": "node ./scripts/postinstall.js",
    "build": "tsc --build tsconfig.build.json && tsc-alias -p tsconfig.build.json && tsx scripts/patch-defineConfig.mts",
    "clean": "rm -rf .dts .turbo dist .eslintcache",
    "lint": "eslint --cache . --max-warnings 0",
    "lint:fix": "eslint --cache . --max-warnings 0 --fix",
    "start": "node dist/cli/index.js",
    "start:dev": "tsx --conditions development src/cli/index.ts",
    "start:server": "tsx --conditions development src/trpc/server.ts",
    "release:alpha": "tsx scripts/release-alpha.mts",
    "release:latest": "tsx scripts/release-latest.mts",
    "type-check": "tsc --build",
    "test": "vitest run src",
    "test:e2e": "vitest run e2e",
    "test:debug": "vitest run",
    "test:watch": "vitest"
  }
}