{
  "name": "@snaplet/seed",
  "type": "module",
  "version": "1.0.0",
  "sideEffects": false,
  "bin": "./bin/cli.js",
  "types": "./dist/index.d.ts",
  "imports": {
    "#*": {
      "development": "./src/*",
      "default": "./dist/*"
    },
    "#test": "./test/index.js",
    "#test/*": "./test/*"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "development": "./src/config.ts",
      "default": "./dist/config.js",
      "types": "./dist/config.d.ts"
    },
    "./cli": {
      "development": "./src/cli/index.ts",
      "default": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./dialects/*": {
      "development": "./src/dialects/*.ts",
      "default": "./dist/dialects/*.js",
      "types": "./dist/dialects/*.d.ts"
    }
  },
  "devDependencies": {
    "@snaplet/copycat": "5.0.0",
    "@snaplet/eslint-config": "workspace:*",
    "@snaplet/tsconfig": "workspace:*",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/better-sqlite3": "7.6.9",
    "@types/fs-extra": "11.0.4",
    "@types/node": "20.11.18",
    "@types/pg": "8.11.0",
    "@types/tmp": "0.2.6",
    "@types/uuid": "9.0.8",
    "@types/yargs": "17.0.32",
    "ansi-colors": "4.1.3",
    "better-sqlite3": "9.4.3",
    "debug": "4.3.4",
    "eslint": "8.56.0",
    "execa": "8.0.1",
    "fs-extra": "11.2.0",
    "postgres": "3.4.3",
    "tmp-promise": "3.0.3",
    "tsx": "4.7.1",
    "typescript": "5.3.3",
    "uuid": "9.0.1",
    "vite-node": "1.3.1",
    "vitest": "1.2.2"
  },
  "peerDependencies": {
    "@snaplet/copycat": ">=2.0.0",
    "drizzle-orm": "^0.29.3"
  },
  "dependencies": {
    "@babel/types": "7.23.9",
    "@scaleleap/pg-format": "^1.0.0",
    "@trpc/client": "10.45.1",
    "@trpc/server": "10.45.1",
    "c12": "1.8.0",
    "find-up": "7.0.0",
    "inflection": "^3.0.0",
    "javascript-stringify": "2.1.0",
    "json-schema-library": "9.1.3",
    "ora": "8.0.1",
    "quicktype-core": "^23.0.104",
    "remeda": "^1.41.0",
    "yargs": "^17.7.2",
    "zod": "3.22.4"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rm -rf .dts .turbo dist .eslintcache",
    "lint": "eslint --cache . --max-warnings 0",
    "lint:fix": "eslint --cache . --max-warnings 0 --fix",
    "start": "node dist/cli/index.js",
    "start:dev": "tsx --conditions development src/cli/index.ts",
    "start:server": "tsx --conditions development src/trpc/server.ts",
    "type-check": "tsc --build",
    "test": "vitest run",
    "test:e2e": "vitest run e2e"
  }
}